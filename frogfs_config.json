{
    "filters": {
        "romfs.paths": "discard",
        "romfs.json": "discard",
        "*.css": "uglifycss",
        "*.html": [
            "html-minifier",
            "gzip"
        ],
        "*.js": [
            "babel-convert",
            "uglifyjs",
            "gzip"
        ],
        "*.txt": "zeroify",
        "*.woff": "uncompressed",
        "*.woff2": "uncompressed",
        "*": [
            "cache",
            "gzip"
        ]
    }
}